{
  "name": "ifiske",
  "version": "3.6.0",
  "description": "ifiske: Enklare Fiskekort",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "gulp --env=development && concurrently -k 'gulp watch --env=development' 'ionic serve'",
    "start.prod": "ionic serve",
    "build": "gulp",
    "build.dev": "gulp --env=development",
    "postinstall": "bower install && ionic state restore",
    "android.dev": "gulp --env=development && ionic run android --device",
    "ios.dev": "gulp --env=development && ionic run ios --device",
    "android": "gulp && ionic run android --device",
    "ios": "gulp && ionic run ios --device",
    "bump": "gulp bump",
    "bump.patch": "gulp bump --patch",
    "bump.minor": "gulp bump --minor",
    "bump.major": "gulp bump --major",
    "release": "gulp release",
    "package": "gulp --env=production && ionic package build android --release -p prod && ionic package build ios --release -p prod",
    "package.android": "gulp --env=production && ionic package build android --release -p prod",
    "package.ios": "gulp --env=production && ionic package build ios --release -p prod",
    "package.ios.download": "gulp --env=production && bash download.sh",
    "resources": "ionic resources",
    "lint": "eslint src",
    "lint.fix": "eslint src --fix",
    "gulp:serve:before": "gulp serve:before",
    "gulp:serve:after": "gulp serve:after",
    "serve:before": "gulp:serve:before",
    "serve:after": "gulp:serve:after",
    "ci.login": "ionic login --email $IONIC_EMAIL --password $IONIC_PASSWORD",
    "update": "npm prune && npm update && bower prune && bower update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifiske/ifiske.git"
  },
  "devDependencies": {
    "@ionic/cli-plugin-cordova": "1.1.2",
