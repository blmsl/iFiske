{
  "name": "ifiske",
  "version": "3.4.2",
  "description": "ifiske: Enklare Fiskekort",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=development|| set NODE_ENV=development&& ionic serve",
    "start.prod": "ionic serve",
    "build": "gulp",
    "build.dev": "gulp --env=development",
    "postinstall": "bower install && ionic state restore",
    "android.dev": "gulp --env=development && ionic run android --device",
    "ios.dev": "gulp --env=development && ionic run ios --device",
    "android": "gulp && ionic run android --device",
    "ios": "gulp && ionic run ios --device",
    "bump": "gulp bump",
    "bump.patch": "gulp bump --patch",
    "bump.minor": "gulp bump --minor",
    "bump.major": "gulp bump --major",
    "package": "gulp --env=production && ionic package build android --release -p prod && ionic package build ios --release -p prod",
    "package.android": "gulp --env=production && ionic package build android --release -p prod",
    "package.ios": "gulp --env=production && ionic package build ios --release -p prod",
    "resources": "ionic resources",
    "lint": "eslint src",
    "lint.fix": "eslint src --fix",
    "gulp:serve:before": "gulp serve:before",
    "gulp:serve:after": "gulp serve:after",
    "serve:before": "gulp:serve:before",
    "serve:after": "gulp:serve:after"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifiske/ifiske.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bower": "^1.7.9",
    "cordova": "^6.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.5.0",
    "eslint-config-google": "0.6.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-cordova-bump": "^1.3.2",
    "gulp-if": "^2.0.1",
    "gulp-json-sort": "^1.0.0",
    "gulp-markdown": "^1.2.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "ionic": "^2.0.0",
    "map-stream": "0.0.6",
    "minimist": "^1.2.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-app-version@0.1.9",
    "cordova-plugin-device@1.1.3",
    "cordova-plugin-file-transfer@1.6.0",
    "cordova-plugin-file@4.3.0",
    "cordova-plugin-geolocation@2.3.0",
    "cordova-plugin-google-analytics@1.7.2",
    "cordova-plugin-inappbrowser@1.5.0",
    "cordova-plugin-screen-orientation@1.4.2",
    "cordova-plugin-splashscreen@4.0.0",
    "cordova-plugin-statusbar@2.2.0",
    "cordova-plugin-whitelist@1.3.0",
    "cordova-plugin-x-toast@2.5.2",
    "cordova-sms-plugin-no-send-sms-permission@0.1.8",
    "ionic-plugin-keyboard@2.2.1",
    "phonegap-plugin-barcodescanner@6.0.3",
    "phonegap-plugin-mobile-accessibility@1.0.4-dev",
    "uk.co.workingedge.phonegap.plugin.launchnavigator@3.2.0",
    {
      "variables": {
        "SENDER_ID": "196216212249"
      },
      "locator": "phonegap-plugin-push@1.8.4"
    }
  ],
  "cordovaPlatforms": [
    "android",
    "ios"
  ],
  "dependencies": {
    "@ionic/cloud": "~0.14.1",
    "angular-chart.js": "^1.1.1",
    "moment": "^2.17.1"
