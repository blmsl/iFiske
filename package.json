{
    "name": "ifiske",
    "version": "3.6.0",
    "description": "ifiske: Enklare Fiskekort",
    "license": "Apache-2.0",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development ionic serve",
        "start.prod": "ionic serve",
        "build": "gulp",
        "build.dev": "gulp --env=development",
        "android.dev": "gulp --env=development && ionic cordova run android --device",
        "ios.dev": "gulp --env=development && ionic cordova run ios --device",
        "android": "gulp && ionic cordova run android --device",
        "ios": "gulp && ionic run ios --device",
        "ionic": "ionic",
        "bump": "gulp bump",
        "bump.patch": "gulp bump --patch",
        "bump.minor": "gulp bump --minor",
        "bump.major": "gulp bump --major",
        "release": "gulp release",
        "package": "ionic cordova resources --no-interactive && gulp --env=production && ionic package build android --release -p prod --no-interactive && ionic package build ios --release -p prod --no-interactive",
        "package.android": "gulp --env=production && ionic package build android --prod --release -p prod --no-interactive",
        "package.ios": "gulp --env=production && ionic package build ios --prod --release -p prod --no-interactive",
        "resources": "ionic cordova resources",
        "lint": "eslint src; sass-lint -v",
        "lint.fix": "eslint src --fix",
        "ci.login": "ionic login --email $IONIC_EMAIL --password $IONIC_PASSWORD --no-interactive",
        "update": "npm prune && npm update && bower prune && bower update"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ifiske/ifiske.git"
    },
    "devDependencies": {
        "@ionic/cli-plugin-cordova": "^1.3.0",
