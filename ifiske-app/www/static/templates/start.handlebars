  <div class="logged-in">
    <div class="button" id="my_cards" ontouchend="Navigate.to('my_cards');">Mina Fiskekort</div>
    <div class="button" id="register_catch" text="Registrera Fångst" />
  </div>
  <div class="not-logged-in">
    <div class="button" id="login" target="login" text="Logga in" />
    <div class="button" id="register" target="register_user" text="Registrera" />
  </div>
</div>
