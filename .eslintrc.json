{
    "extends": "google",
    "env": {
        "browser" :true
    },
    "globals": {
        "goog": null,
        "angular": false
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": ["error",
            {
                "code": 100,
                "ignoreComments": true
            }
        ],
        "comma-dangle": ["error", "always-multiline"],
        "key-spacing": ["error",
            {
                "align": "value"
            }
        ],
        "radix": "off",
        "one-var": ["error",
            {
                "uninitialized": "always",
                "initialized": "never"
            }
        ]
    }
}
