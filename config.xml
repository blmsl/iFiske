<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="se.iFiske.fiskekort" android-packageName="se.iFiske.fiskekort" ios-CFBundleIdentifier="se.iFiske.iFiske" version="3.3.5">
  <name>iFiske Fiskekort</name>
  <description>iFiske - Enklare Fiskekort!</description>
  <author email="app@ifiske.se" href="http://www.ifiske.se">iFiske</author>
  <content src="index.html"/>
  <preference name="android-minSdkVersion" value="14"/>
  <preference name="android-targetSdkVersion" value="22"/>
  <preference name="orientation" value="portrait"/>
  <preference name="SplashScreenDelay" value="2000"/>
  <preference name="SplashScreenBackgroundColor" value="white"/>
  <preference name="FadeSplashScreenDuration" value="250"/>
  <preference name="target-device" value="universal"/>
  <access origin="*"/>
  <access origin="cdvfile://*"/>
  <allow-navigation href="cdvfile://*"/>
  <allow-navigation href="gap://ready"/>
  <allow-intent href="cdvfile://*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <plugin name="cordova-plugin-app-version" spec="1.0.0"/>
  <plugin name="cordova-plugin-device" spec="1.1.2"/>
  <plugin name="cordova-plugin-file-transfer" spec="1.5.1"/>
  <plugin name="cordova-plugin-file" spec="4.2.0"/>
  <plugin name="cordova-plugin-geolocation" spec="2.2.0"/>
  <plugin name="cordova-plugin-inappbrowser" spec="1.4.0"/>
  <plugin name="cordova-plugin-splashscreen" spec="3.2.2"/>
  <plugin name="cordova-plugin-statusbar" spec="2.1.3"/>
  <plugin name="cordova-plugin-whitelist" spec="1.2.2"/>
  <plugin name="cordova-plugin-x-toast" spec="2.5.1"/>
  <plugin name="cordova-sms-plugin" spec="0.1.10"/>
  <plugin name="ionic-plugin-keyboard" spec="2.0.1"/>
  <plugin name="phonegap-plugin-mobile-accessibility" spec="1.0.4-dev"/>
  <plugin name="phonegap-plugin-push" spec="1.6.3"/>
  <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" spec="3.0.4"/>
</widget>
